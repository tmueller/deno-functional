{
  "compilerOptions": {
    "allowJs": true,
    "baseUrl": ".",
    "esModuleInterop": true,
    "lib": ["esnext"],
    "module": "amd",
    "moduleResolution": "node",
    "noEmit": true,
    "paths": {
      "http://*": [
        "~/.cache/deno/deps/http/*"
      ],
      "https://*": [
        "~/.cache/deno/deps/https/*"
      ],
      "deno": [
        "./lib/lib.deno_runtime.d.ts"
      ],
    },
    "plugins": [
      {
        "name": "deno_ls_plugin"
      }
    ],
    "pretty": true,
    "resolveJsonModule": true,
    "target": "esnext"
  },
  "include": ["./**/*.ts"]
}
